<?xml version="1.0" encoding="utf-8"?>

<!DOCTYPE urlrewrite
        PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">

<urlrewrite>

    <!-- Handle HTML5-style client routes. -->
    <rule match-type="wildcard">
        <from>/views/**</from>
        <to last="true">/index.html</to>
    </rule>

</urlrewrite>
